<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
	<diskStore path="java.io.tmpdir" />
	<defaultCache maxElementsInMemory="5000" eternal="false"
		timeToIdleSeconds="5000" timeToLiveSeconds="5000" overflowToDisk="false" />
	<cache name="sampleCache" maxElementsInMemory="5000" eternal="false"
		timeToIdleSeconds="1" timeToLiveSeconds="1" overflowToDisk="false" />
	<cache name=".*users:.*" maxElementsInMemory="5000" eternal="false"
		timeToIdleSeconds="50" timeToLiveSeconds="60" overflowToDisk="false" />
</ehcache>

